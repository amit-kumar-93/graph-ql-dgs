type Query {
 shows(titleFilter: String): [Show]
 stocks: Stock
}

type Show{
title: String
releaseYear: Int
reviews: [Review]
directors: [Director]
}

type Review{
starRating: Int
}

type Director{
name: String
rating: Int
showsDirected: Int
}

type Subscription {
    stocks: Stock
}

type Stock {
    name: String
    price: Float
}